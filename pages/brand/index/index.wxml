<view class="container">
	<view class="main-container">
		<!-- 轮播图 -->
		<view class="banner-swiper-container">
			<view>
				<view class="infoCenter">
					<view>
						<image style="width:80rpx;height:80rpx;" src="{{logoUrl}}"></image>
					</view>
					<view class="infoText"> {{brandName}}4S店 </view>
				</view>
			</view>
			<!-- 广告 -->
		</view>
	</view>
</view>
<view class="list-container">
	<view
	 class="list-item box-shadow"
	 wx:for="{{activityList}}"
	 wx:key="index"
	>
		<image
		 src="{{item.viewImage}}"
		 bindtap="shopping"
		 data-operation="{{item}}"
		></image>
	</view>
</view>

