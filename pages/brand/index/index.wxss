.container{
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.back{
    display: flex;
    position: relative;
    font-size: 24rpx;
    font-weight: 400;
    height: 60rpx;
    top: 0px;
    left: 0px;
    margin-top: -30rpx;
    line-height: 60rpx;
    color: #444444;
    font-family:Source Han Sans CN;
  }
  image{
    padding-top: 9rpx;
    margin-right: 16rpx;
  }
  .back image{
    width: 40rpx;
  }
  .back text{
    font-size:24rpx;
    font-family:Source Han Sans CN;
    font-weight:400;
    color:rgba(68,68,68,1);
  }
  .imageRight{
    position:absolute;
    right: 0px;
    top: 0px;
  }
  
  .tab-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  
  .tab-list {
    font-size: 28rpx;
    font-weight: bold;
    width: 400rpx;
    position: relative;
    display: inline-block;
  }
  .image{
    position: absolute;
    top: 15rpx;
    display: inline-block;
    width: 40rpx;
    left: 0px;
  }
  .tab-active {
    color: #ee731a !important;
    font-size: 36rpx !important;
    font-weight: bold !important;
  }
  .banner-swiper-container .swipers {
    width: 100%;
    height: 446rpx;
  }
  .banner-swiper-container .swipers image {
    width: 100%;
    height: 100%;
  }
  .infoCenter{
    display: flex ;
    margin-top: 20rpx;
    align-items: center;
    justify-content: center;
    text-align: center;
    line-height: 40rpx;
  }
  .infoText{
    display: flex;
    color: #444444;
    width:206rpx;
    height:26rpx;
    font-size:28rpx;
    font-family:Source Han Sans CN;
    font-weight:500;
    color:rgba(68,68,68,1);
  }
  .mt0 {
    margin-top: 0rpx !important;
  }
  